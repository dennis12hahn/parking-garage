<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.parkinggarage">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controller.action_controllers.ParkActivity"></activity>
        <activity android:name=".controller.user_controllers.AttendantActivity" />
        <activity android:name=".controller.user_controllers.ManagerActivity" />
        <activity android:name=".controller.Manager" />
        <activity android:name=".controller.action_controllers.CreateGarageActivity" />
        <activity android:name=".controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controller.action_controllers.SignInActivity" />
        <activity android:name=".controller.action_controllers.CreateAttendantActivity" />
    </application>

</manifest>